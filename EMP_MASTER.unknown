CREATE TABLE  "EMP_MASTER" 
   (	"CONTROL_NO" VARCHAR2(20), 
	"EMP_NAME" VARCHAR2(100) NOT NULL ENABLE, 
	"DEPARTMENT" VARCHAR2(100), 
	"DESIGNATION" VARCHAR2(100), 
	"PHONE" VARCHAR2(15), 
	"DATE_OF_BIRTH" DATE, 
	"DATE_OF_APPOINTMENT" DATE, 
	"BILL_UNIT" VARCHAR2(50), 
	 PRIMARY KEY ("CONTROL_NO") ENABLE
   )